{
  "name": "blog-notify",
  "description": "Scan for new blog posts to send email campaigns",
  "version": "0.0.0",
  "author": "Matthew Castrillon-Madrigal @matthewcm",
  "bin": {
    "blog-notify": "./bin/run"
  },
  "bugs": "https://github.com/matthewcm/blog-notify/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/errors": "^1.3.4",
    "@oclif/plugin-help": "^3",
    "@sendgrid/mail": "^7.4.2",
    "chalk": "^4.1.1",
    "cli-progress": "^3.9.0",
    "dotenv": "^8.2.0",
    "gray-matter": "^4.0.3",
    "inquirer": "^8.0.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/chalk": "^2.2.0",
    "@types/cli-progress": "^3.9.1",
    "@types/inquirer": "^7.3.1",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/matthewcm/blog-notify",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "blog-notify",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "matthewcm/blog-notify",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
